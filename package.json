{
  "name": "magne.github.io",
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "postbuild": "pagefind --site dist",
    "sync": "astro sync",
    "preview": "astro preview",
    "biome": "biome check .*.cjs *.cjs *.ts *.json src",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,cjs,mjs,astro}\"",
    "format": "prettier --write  \"**/*.{js,jsx,ts,tsx,cjs,mjs,md,mdx,astro,json}\" && eslint --fix \"src/**/*.{js,ts,jsx,tsx,cjs,mjs,astro}\"",
    "pre-commit": "lint-staged",
    "prepare": "husky"
  },
  "dependencies": {
    "motion": "^10.17.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.5.6",
    "@astrojs/mdx": "^2.1.1",
    "@astrojs/rss": "^4.0.5",
    "@astrojs/sitemap": "^3.1.1",
    "@astrojs/tailwind": "^5.1.0",
    "@biomejs/biome": "^1.5.3",
    "@pagefind/default-ui": "^1.0.4",
    "@tailwindcss/typography": "^0.5.10",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.25",
    "@typescript-eslint/parser": "^7.1.1",
    "astro": "^4.4.9",
    "astro-font": "^0.0.77",
    "clsx": "^2.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^0.31.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "luxon": "^3.4.4",
    "mdast-util-to-string": "^4.0.0",
    "pagefind": "^1.0.4",
    "prettier": "^3.2.5",
    "prettier-config-standard": "^7.0.0",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "reading-time": "^1.5.0",
    "rehype-mathjax": "^6.0.0",
    "rehype-pretty-code": "^0.13.0",
    "remark-math": "^6.0.0",
    "sharp": "^0.33.2",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs,astro,json}": "eslint --fix",
    "*.{js,jsx,ts,tsx,cjs,mjs,md,mdx,astro}": "prettier --write"
  }
}
