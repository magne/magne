{
  "name": "magne.github.io",
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "postbuild": "pagefind --site dist",
    "sync": "astro sync",
    "preview": "astro preview",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,cjs,mjs,astro}\"",
    "format": "prettier --write  \"**/*.{js,jsx,ts,tsx,cjs,mjs,md,mdx,astro,json}\" && eslint --fix \"src/**/*.{js,ts,jsx,tsx,cjs,mjs,astro}\"",
    "pre-commit": "lint-staged",
    "prepare": "husky"
  },
  "dependencies": {
    "motion": "^10.17.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/mdx": "^3.0.0",
    "@astrojs/rss": "^4.0.6",
    "@astrojs/sitemap": "^3.1.5",
    "@astrojs/tailwind": "^5.1.0",
    "@biomejs/biome": "^1.7.3",
    "@pagefind/default-ui": "^1.1.0",
    "@tailwindcss/typography": "^0.5.13",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.12.12",
    "@typescript-eslint/parser": "^7.9.0",
    "astro": "^4.8.4",
    "astro-font": "^0.0.81",
    "clsx": "^2.1.1",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^1.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "luxon": "^3.4.4",
    "mdast-util-to-string": "^4.0.0",
    "pagefind": "^1.1.0",
    "prettier": "^3.2.5",
    "prettier-config-standard": "^7.0.0",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "reading-time": "^1.5.0",
    "rehype-mathjax": "^6.0.0",
    "rehype-pretty-code": "^0.13.2",
    "remark-math": "^6.0.0",
    "sharp": "^0.33.4",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs,astro,json}": "eslint --fix",
    "*.{js,jsx,ts,tsx,cjs,mjs,md,mdx,astro}": "prettier --write"
  }
}
